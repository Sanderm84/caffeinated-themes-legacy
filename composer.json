{
    "name": "caffeinated/themes",
    "description": "Laravel Themes",
    "keywords": ["themes", "laravel", "caffeinated", "blade"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shea Lewis",
            "email": "shea.lewis89@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.4|^7.3|^8.0",
        "illuminate/support": "5.3.*|5.4.*|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/filesystem": "5.3.*|5.4.*|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/view": "5.3.*|5.4.*|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Caffeinated\\Themes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caffeinated\\Themes\\ThemesServiceProvider"
            ],
            "aliases": {
                "Theme": "Caffeinated\\Themes\\Facades\\Theme"
            }
        }
    }
}
